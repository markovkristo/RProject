library(stringr)
firmaTulp <- function(netflix, hulu, disney, amazon){
abi <- c()
if(netflix == 1){
append(abi, "Netflix")
}
if(hulu == 1){
append(abi, "Hulu")
}
if(amazon == 1){
append(abi, "Amazon")
}
if(disney == 1){
append(abi, "Disney")
}
return abi
}
